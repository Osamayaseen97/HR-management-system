// Calculate the salary for each employee
  function calculateSalary(level) {
    let min, max;
    
    if (level === "Senior") {
      min = 1500;
      max = 2000;
    } else if (level === "Mid-Senior") {
      min = 1000;
      max = 1500;
    } else if (level === "Junior") {
      min = 500;
      max = 1000;
    }
    
    const salary = Math.floor(min + Math.random() * (max - min + 1));
    const netSalary = salary - (salary * 0.075); // Calculate net salary after tax
    
    return netSalary;
  }
  
